# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
XAI_API_KEY=your_xai_api_key_here
ALIBABA_API_KEY=your_alibaba_api_key_here

# 日志配置
LOG_LEVEL=INFO

# Prompt 配置
# PROMPT_FILE_PATH=prompt/youtbe-deep-summary.txt

# 模型配置
PREFERRED_MODEL=Gemini

# 认证配置
ADMIN_USERNAME=admin
ADMIN_PASSWORD=password

# Cookie Manager 配置
# Cookie 刷新间隔（小时）
# 默认: 6 小时
# 建议范围: 4-12 小时
# - 设置太短（<4小时）可能被 YouTube 检测为异常
# - 设置太长（>12小时）可能导致 cookies 过期
COOKIE_REFRESH_INTERVAL=6

# 浏览器类型 (chromium, firefox, webkit)
# 默认: chromium（推荐）
COOKIE_BROWSER_TYPE=chromium
