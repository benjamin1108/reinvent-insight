# API Keys
GEMINI_API_KEY=your-gemini-api-key-here
DASHSCOPE_API_KEY=your-dashscope-api-key-here
XAI_API_KEY=your-xai-api-key-here
ALIBABA_API_KEY=your-alibaba-api-key-here

# 日志配置
LOG_LEVEL=INFO

# 模型配置（可选，会覆盖配置文件）
# PREFERRED_MODEL=Gemini

# Cookie 配置
COOKIE_REFRESH_INTERVAL=6
COOKIE_BROWSER_TYPE=chromium

# 文件大小限制
MAX_TEXT_FILE_SIZE=10485760
MAX_BINARY_FILE_SIZE=52428800

# 可视化解读
VISUAL_INTERPRETATION_ENABLED=true

# 关键帧截图配置
# 是否启用YouTube视频关键帧截图功能
ENABLE_KEYFRAME_SCREENSHOT=false
# 最小章节数（低于此值不生成截图）
KEYFRAME_MIN_CHAPTER_COUNT=5
# 最大截图数量
KEYFRAME_MAX_COUNT=8
# 截图分辨率
KEYFRAME_SCREENSHOT_WIDTH=1920
KEYFRAME_SCREENSHOT_HEIGHT=1080
# 等待时间（秒，视频缓冲稳定）
KEYFRAME_WAIT_TIME=3
# 单个截图超时时间（秒）
KEYFRAME_TIMEOUT=30

# Visual Long Image 配置
# 是否启用长图生成功能
VISUAL_LONG_IMAGE_ENABLED=true
# 截图视口宽度（像素，默认 1080px 适合移动端分享，桶面端可设为 1920）
VISUAL_SCREENSHOT_VIEWPORT_WIDTH=1080
# 截图等待时间（秒，用于等待图表渲染）
VISUAL_SCREENSHOT_WAIT_TIME=3
# 浏览器启动超时（秒）
VISUAL_SCREENSHOT_BROWSER_TIMEOUT=30

# TTS 音频播放配置
# 是否显示音频播放按钮（true=显示, false=隐藏）
TTS_AUDIO_BUTTON_ENABLED=true

# 认证配置
ADMIN_USERNAME=admin
ADMIN_PASSWORD=password

# ========================================
# 任务队列配置（Worker Pool）
# ========================================

# 最大并发分析任务数（同时运行的 worker 数量）
# 建议值：2-5，根据服务器性能和 API 配额调整
# 过高可能触发 API 限流，过低影响处理速度
MAX_CONCURRENT_ANALYSIS_TASKS=1

# 任务队列最大长度（等待中的任务数量限制）
# 超过此限制时，新任务将被拒绝（返回 503 错误）
# 建议值：50-200，根据预期负载调整
ANALYSIS_QUEUE_MAX_SIZE=100

# 单个分析任务的最大执行时间（秒）
# 超时任务将被自动终止
# 建议值：1800-7200（30分钟-2小时）
ANALYSIS_TASK_TIMEOUT=1800
